# Discord 配置
DISCORD_TOKEN=your_discord_token_here
TARGET_CHANNEL_ID=your_channel_id_here

# 音频处理配置
AUDIO_SAMPLE_RATE=44100
AUDIO_CHANNELS=2
AUDIO_BUFFER_SIZE=4096
AUDIO_QUEUE_SIZE=100

# 后台管理配置
MAX_THREADS=4
MAX_CONCURRENT_TASKS=10
CPU_THRESHOLD=80.0
MEMORY_THRESHOLD=80.0

# 音频效果默认参数
DEFAULT_VOLUME=1.0
DEFAULT_DISTORTION=0.0
DEFAULT_CHORUS_RATE=1.0
DEFAULT_CHORUS_DEPTH=0.5
DEFAULT_CHORUS_MIX=0.5
DEFAULT_FLANGER_RATE=1.0
DEFAULT_FLANGER_DEPTH=0.5
DEFAULT_FLANGER_MIX=0.5
DEFAULT_PHASER_RATE=1.0
DEFAULT_PHASER_DEPTH=0.5
DEFAULT_PHASER_MIX=0.5
DEFAULT_WAH_FREQ=1.0
DEFAULT_WAH_Q=1.0
DEFAULT_WAH_MIX=0.5

# 系统资源限制
MAX_MEMORY_USAGE=512MB
MAX_CPU_USAGE=80.0%
MAX_DISK_USAGE=1GB

# 缓存配置
CACHE_DIR=~/.cache/discord-music-bot
MAX_CACHE_SIZE=100MB
CACHE_EXPIRY=24h

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=~/.local/share/discord-music-bot/logs/bot.log
MAX_LOG_SIZE=10MB
MAX_LOG_FILES=5

# 网络配置
PROXY_ENABLED=false
PROXY_URL=
PROXY_USERNAME=
PROXY_PASSWORD=
TIMEOUT=30

# 安全配置
ENCRYPTION_KEY=your_encryption_key_here
API_RATE_LIMIT=100
MAX_CONNECTIONS=50 